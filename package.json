{
  "name": "aws-serverless-prototype",
  "version": "0.1.0",
  "description": "Serverless CRUD Prototype",
  "scripts": {
    "test": "mocha -t 30000 -R nyan --compilers ts:ts-node/register",
    "test:local": "IS_LOCAL=true mocha -t 30000 --compilers ts:ts-node/register",
    "test:coverage": "IS_LOCAL=true nyc --extension .ts mocha -t 30000 --compilers ts:ts-node/register",
    "coverage:report:html": "nyc report --reporter=html",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "build": "webpack",
    "migrate": "serverless dynamodb migrate",
    "server": "serverless offline --location .webpack",
    "deploy": "serverless deploy"
  },
  "author": "keita-nishimoto",
  "license": "MIT",
  "dependencies": {
    "@types/aws-lambda": "^0.0.7",
    "@types/aws-sdk": "^0.0.42",
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.8",
    "@types/source-map": "^0.5.0",
    "@types/source-map-support": "^0.2.28",
    "@types/uuid": "^2.0.29",
    "aws-sdk": "^2.26.0",
    "axios": "^0.15.3",
    "chai": "^3.5.0",
    "json-loader": "^0.5.4",
    "jsonschema": "^1.1.1",
    "mocha": "^3.2.0",
    "node-inspector": "^0.12.10",
    "nyc": "^10.1.2",
    "serverless": "^1.9.0",
    "serverless-dynamodb-local": "^0.2.19",
    "serverless-offline": "^3.10.3",
    "serverless-webpack": "^1.0.0-rc.4",
    "source-map-support": "^0.4.11",
    "ts-loader": "^2.0.2",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "uuid": "^3.0.1",
    "webpack": "^1.14.0",
    "webpack-node-externals": "^1.5.4"
  }
}
